{
  "name": "get-pr",
  "version": "0.1.0",
  "description": "Github Action for getting the current PR if the commit belongs to one",
  "main": "lib/main.js",
  "scripts": {
    "format": "prettier --write **/*.ts",
    "lint": "eslint --fix src/**/*.ts",
    "lint:check": "eslint src/**/*.ts",
    "build": "ncc build src/main.ts --source-map --license licenses.txt",
    "test": "jest",
    "format-check": "prettier --check '**/*.ts'",
    "all": "npm run build && npm run format && npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloudposse-github-actions/get-pr.git"
  },
  "keywords": [
    "Github",
    "Action",
    "Javascript"
  ],
  "author": "8BitJonny",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/cloudposse-github-actions/get-pr/issues"
  },
  "homepage": "https://github.com/cloudposse-github-actions/get-pr/issues#readme",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.4.4",
    "@typescript-eslint/parser": "^5.52.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.45.0",
    "eslint-plugin-github": "^4.7.0",
    "eslint-plugin-jest": "^27.2.3",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "prettier": "2.8.8",
    "ts-jest": "^27.1.4",
    "typescript": "^4.9.5"
  }
}
